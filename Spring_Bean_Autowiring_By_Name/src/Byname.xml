<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

<bean id="emp" class="com.name.Employee" autowire="byName">
<property name="Id" value="101"/>
<property name="name" value="Abhishek"/>
</bean>

<bean id="add" class="com.name.Address">
<property name="city" value="Indore"/>
<property name="state" value="Madhya_Pradesh"/>
</bean>

<bean id="add1" class="com.name.Address">
<property name="city" value="Indore1"/>
<property name="state" value="Madhya_Pradesh1"/>
</bean>

<bean id="comp" class="com.name.Company">
<property name="compid" value="5001"/>
</bean>

</beans>